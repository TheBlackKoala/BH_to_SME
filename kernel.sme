type vdata: i32;
type tdata: { val: vdata; };

proc func0()
	bus a3 {
		val: vdata;
	};

	bus a2 {
		val: vdata;
	};

{
	a2.val = a3.val + a3.val;
}

proc func1()
	bus a2 {
		val: vdata;
	};

	bus a1 {
		val: vdata;
	};

{
	a1.val = a2.val + 2;
}

network bohrium(in a3: tdata, out a1: tdata){
	instance 0_inst of func0();
	instance 1_inst of func1();

	connect
		a3.val -> 0_inst.a3.val,
		1_inst.a1.val -> a1.val;
}
